{
  "name": "@polkadot/extension-inject",
  "description": "A generic injector (usable to any extension), that populates the base exposed interfaces to be used by dapps.",
  "version": "0.37.1",
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "license": "Apache-2",
  "main": "index.js",
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.12.13"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.129",
    "@types/firefox-webext-browser": "^82.0.0"
  },
  "peerDependencies": {
    "@polkadot/api": "*"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "./chrome": {
      "import": "./chrome.mjs",
      "default": "./chrome.js"
    },
    "./chrome.d.ts": "./chrome.d.ts",
    "./index.d.ts": "./index.d.ts",
    "./package-info.json": "./package-info.json",
    "./package.json": "./package.json",
    "./README.md": "./README.md",
    "./types": {
      "import": "./types.mjs",
      "default": "./types.js"
    },
    "./types.d.ts": "./types.d.ts"
  }
}